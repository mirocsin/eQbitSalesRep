    <script>
        $("#pgmem_orderentry").on("pagebeforeshow", function(event, ui) {
            pgmem_orderentry_pageInit();
            insertDialogs(this);
            event.preventDefault();
            event.stopImmediatePropagation();
        });
        $("#pgmem_orderentry").on("pageshow", function(event, ui) {
            $(window).resize();
